<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.boomino.launcher.ui.MainFragment.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_mainFragment_to_packageListFragment"
            app:destination="@id/packageListFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_up"
            />
    </fragment>
    <fragment
        android:id="@+id/packageListFragment"
        android:name="com.boomino.launcher.ui.PackageListFragment.PackageListFragment"
        android:label="PackageListFragment" >

    </fragment>
</navigation>